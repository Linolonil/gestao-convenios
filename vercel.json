{
    "version": 2,
    "builds": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "buildCommand": "npm run build",
            "outputDirectory": "dist",
            "rewrites": [
                {
                    "source": "/(.*)",
                    "destination": "/index.html"
                }
            ]
        }
    ]
}